class Solution {
public:
    vector<string> validateCoupons(vector<string>& code, vector<string>& bL, vector<bool>& isActive) {
        int n = code.size();

        vector<string> ans;
        vector<pair<string,string>> mp;
        for(int i=0; i<n; i++){
            if(isActive[i]){
                if(bL[i] == "electronics" || bL[i] == "grocery" || bL[i] == "pharmacy" || bL[i] == "restaurant"){
                    bool flag = false;
                    for(char c : code[i]){
                        if((c >= 'a'  && c <= 'z') || (c >= 'A' && c <= 'Z') || (c == '_') || (c >= '0' && c <= '9')){
                            flag = true;
                        }
                        else{
                            flag = false;
                            break;
                        }
                    }
                    if(flag){
                        mp.push_back({bL[i],code[i]});
                    }
                }
            }
        }
        sort(mp.begin(),mp.end());
        for(auto it : mp){
            ans.push_back(it.second);
        }

        return ans;
    }
};
