class Solution {
public:
    int MOD = 1e9+7;
    int countPermutations(vector<int>& complexity) {
        int n = complexity.size();
        long long fact = 1;

        for(int i=1; i<n; i++){
            if(complexity[0] >= complexity[i]){
                return 0;
            }
            fact = ((fact%MOD) * (i%MOD)) % MOD;
        }

        return fact;
    }
};
