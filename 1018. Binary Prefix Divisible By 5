class Solution {
public:
    vector<bool> prefixesDivBy5(vector<int>& nums) {
        int n = nums.size();
        vector<bool> ans(n, false);
        int t = 0;

        for (int i = 0; i < n; i++) {
            t = (t * 2 + nums[i]) % 5;
            ans[i] = (t == 0);
        }

        return ans;
    }
};
