class Solution {
public:
    int countOperations(int num1, int num2) {
        int opr = 0;
        while(num1 && num2){
            if(num1 >= num2){
                num1 -= num2;
            }
            else{
                num2 -= num1;
            }

            opr++;
        }

        return opr;
    }
};


/////////////////////Using Euclidean Algorithm/////////////////////////
class Solution {
public:
    int countOperations(int num1, int num2) {
        int cnt = 0, a = max(num1, num2), b = min(num1, num2);
        while (a && b) {
            cnt += a / b;
            a %= b;
            swap(a, b);
        }
        return cnt;
    }
};
