class Solution {
public:
    int maximum69Number (int num) {
        string t = to_string(num);
        int n = t.size();
        for(int i=0; i<n; i++){
            if(t[i] == '6'){
                t[i] = '9';
                break;
            }
        }

        num = stoi(t);
        return num;
    }
};
